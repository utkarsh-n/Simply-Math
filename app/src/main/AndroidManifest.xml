<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.youdothemath">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/First">
        <activity android:name=".MainActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PlayGame">
        </activity>
        <activity android:name=".HowToPlay"
            android:theme="@style/Tables">
        </activity>
        <activity android:name=".HighScores"
            android:theme="@style/Scores">
        </activity>
        <activity android:name=".About"
            android:theme="@style/About"/>
        <activity android:name=".Practice"
            android:theme="@style/AppTheme" />
        <activity android:name=".Addition"
        android:theme="@style/Addition">
        </activity>
        <activity android:name=".Subtraction"
            android:theme="@style/Subtraction">
        </activity>
        <activity android:name=".Multiplication"
            android:theme="@style/Multiplication">
        </activity>
        <activity android:name=".Division"
            android:theme="@style/Division">
        </activity>
        <activity android:name=".All"
            android:theme="@style/All">
        </activity>
        <activity android:name=".SquareRoot"
            android:theme="@style/Sqrt">
        </activity>
    </application>

</manifest>